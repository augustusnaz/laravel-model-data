{
    "name": "moirei/laravel-model-data",
    "description": "Easily handle data fields in your Eloquent models",
    "keywords": [
        "moirei",
        "laravel",
        "model",
        "meta",
        "data",
        "eloquent",
        "extra data",
        "model data"
    ],
    "homepage": "https://github.com/augustusnaz/laravel-model-data",
    "license": "MIT",
    "authors": [
        {
            "name": "Augustus Okoye",
            "email": "augustusokoye@moirei.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/database": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "MOIREI\\ModelData\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MOIREI\\ModelData\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MOIREI\\ModelData\\ModelDataServiceProvider"
            ]
        }
    }
}
